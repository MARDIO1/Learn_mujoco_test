<mujoco model="plane">
<!-- 模型统计信息 -->
    <statistic center="0 0 0" extent="2"/>
    <!-- 物理世界 -->
    <worldbody>        
        <!-- 飞镖主体 -->
        <body name="missile" pos="0 0 0.4" quat="0.9659258262890683 0 -0.25881904510252074 0">
            
            <!-- 飞镖主体几何体 - 方块 -->
            <geom name="cart_body" 
                  type="box" 
                  size="0.2 0.1 0.05"
                  mass="1.0"
                  material="cart_material"
                  friction="0.8 0.1 0.1"/>

            <!-- 飞镖关节 - 水平运动自由度 -->
            <joint name="missile_main_joint" type="free"/>
            <site name="IMU_site" pos="0 0 0" size="0.01" rgba="0 1 0 1"/>
        </body>
        
    </worldbody>
    <sensor>
        <accelerometer site="IMU_site"/>
        <gyro          site="IMU_site"/>
    </sensor>
</mujoco>